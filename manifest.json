{
	"manifest_version": 2,
	"name": "Window Titler",
	"version": "1.1.1",
	"description": "Lets you add a custom per-window title. Useful if you like to have topically themed Firefox windows.",
	"browser_action": {
		"default_title": "Window titler",
		"default_popup": "popup.html",
		"default_icon": "icons/dark/icon-32.png",
		"theme_icons": [{
			"light": "icons/light/icon-w-16.png",
			"dark": "icons/dark/icon-16.png",
			"size": 16
			}, {
			"light": "icons/light/icon-w-24.png",
			"dark": "icons/dark/icon-24.png",
			"size": 24
			}, {
			"light": "icons/light/icon-w-32.png",
			"dark": "icons/dark/icon-32.png",
			"size": 32
		  }, {
			"light": "icons/light/icon-w-48.png",
			"dark": "icons/dark/icon-48.png",
			"size": 48
		  }, {
			"light": "icons/light/icon-w-64.png",
			"dark": "icons/dark/icon-64.png",
			"size": 64
		  }, {
			"light": "icons/light/icon-w-96.png",
			"dark": "icons/dark/icon-96.png",
			"size": 96
		  }, {
			"light": "icons/light/icon-w-128.png",
			"dark": "icons/dark/icon-128.png",
			"size": 128
			}, {
			"light": "icons/light/icon-w-256.png",
			"dark": "icons/dark/icon-256.png",
			"size": 256
			}]
	},
	"commands": {
        "_execute_browser_action": {
            "suggested_key": {
                "default": "Alt+Ctrl+T"
            }
        }
    },
	"icons": {
		"16": "icons/dark/icon-16.png",
		"24": "icons/dark/icon-24.png",
		"32": "icons/dark/icon-32.png",
		"48": "icons/dark/icon-48.png",
		"64": "icons/dark/icon-64.png",
		"96": "icons/dark/icon-96.png",
		"128": "icons/dark/icon-128.png",
		"256": "icons/dark/icon-256.png"
	}
}
