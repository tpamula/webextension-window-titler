{
	"manifest_version": 2,
	"name": "Window Titler",
	"version": "1.0.4",
	"description": "Lets you add a custom per-window title. Useful if you like to have topically themed Firefox windows.",
	"browser_action": {
		"default_title": "Window titler",
		"default_popup": "popup.html",
		"default_icon": "icons/icon-transparent-32.png"
	},
	"commands": {
        "_execute_browser_action": {
            "suggested_key": {
                "default": "Alt+Ctrl+T"
            }
        }
    },
	"icons": {
		"48": "icons/icon-48.png",
		"96": "icons/icon-96.png"
	}
}
